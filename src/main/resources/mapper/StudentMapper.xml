<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wx.springBootShiro.shrio.mapper.StudentMapper">
    <resultMap id="BaseResultMap" type="com.wx.springBootShiro.shrio.model.pojo.Student">
        <id column="SNO" jdbcType="VARCHAR" property="sno" />
        <result column="SNAME" jdbcType="VARCHAR" property="name" />
        <result column="SSEX" jdbcType="VARCHAR" property="sex" />
    </resultMap>

    <select id="queryAll" resultMap="BaseResultMap">
        select * from student
    </select>
</mapper>